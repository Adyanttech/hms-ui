{
  "name": "hospital_management",
  "version": "0.1.0",
  "main": "server.ts",
  "repository": "https://github.com/Adyanttech/hms-ui.git",
  "author": "Vinay Kumar <vinaykumarchakka@gmail.com>",
  "license": "MIT",
 "scripts": {
    "build": "NEXT_TELEMETRY_DISABLED=1 yarn build:client && yarn build:server",
    "build:production": "yarn run build && ./bin/prepare_prod",
    "build:client": "NEXT_TELEMETRY_DISABLED=1 next build ./src/client",
    "build:server": "tsc -p tsconfig.server.json",
    "dev": "NEXT_TELEMETRY_DISABLED=1 ts-node-dev --ignore-watch ./src/client --transpile-only ./src/main.ts",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fmt": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write \"**/*.{json,md,yml,ts,tsx,js,jsx}\"",
    "prettier": "prettier -c \"**/*.{md,json,yml,sh}\"",
    "start": "NEXT_TELEMETRY_DISABLED=1 node ./dist/main.js"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "fastify": "^3.25.3",
    "fastify-plugin": "^3.0.0",
    "next": "^12.0.7",
    "pino-pretty": "^7.3.0",
    "primeicons": "^6.0.1",
    "primereact": "^10.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^8.0.5",
    "sass": "^1.58.3"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^5.8.3"
  }
}
