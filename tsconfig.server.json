{
    "compilerOptions": {
      "target": "esnext",
      "lib": ["esnext"],
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": false,
      "incremental": true
    },
    "include": ["src/**/*.ts", "server.ts"],
    "exclude": ["node_modules", "src/client/**/*"]
  }
  